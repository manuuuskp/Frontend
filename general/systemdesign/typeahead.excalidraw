      "roughness": 1,
      "opacity": 100,
      "angle": 0,
      "x": 7050.25911128603,
      "y": 8403.743261834174,
      "strokeColor": "#1e1e1e",
      "backgroundColor": "#ffffff",
      "width": 3121.875,
