    {
      "type": "text",
      "version": 9287,
      "versionNonce": 1314874950,
      "isDeleted": false,
      "id": "VR6F9cT8-gk3NMPW513YJ",
      "fillStyle": "solid",
      "strokeWidth": 2,
      "strokeStyle": "solid",
      "roughness": 1,
      "opacity": 100,
      "angle": 0,
      "x": 345.63874573313194,
      "y": 7766.679862190507,
      "strokeColor": "#1e1e1e",
      "backgroundColor": "transparent",
      "width": 1675.78125,
      "height": 4272,
      "seed": 1283368600,
      "groupIds": [],
